<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://www.aocat.net/NT/v4.0"
	targetNamespace="http://www.aocat.net/NT/v4.0"
	elementFormDefault="qualified">

	<xs:include schemaLocation="Resposta.xsd"/>

	<xs:element name="RespostaReport">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Resposta indicant que el report s'ha rebut correctament.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="Error" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
