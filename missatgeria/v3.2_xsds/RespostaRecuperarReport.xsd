<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://www.aocat.net/NT/v3.2"
	targetNamespace="http://www.aocat.net/NT/v3.2"
	elementFormDefault="qualified"
	attributeFormDefault="unqualified">

	<xs:include schemaLocation="Resposta.xsd"/>
	<xs:include schemaLocation="Report.xsd"/>

	<xs:element name="RespostaRecuperarReport" type="RespostaRecuperarReportType"/>
	<xs:complexType name="RespostaRecuperarReportType">
		<xs:annotation>
			<xs:documentation>Dades del report de notificacions</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RespostaType">
                <xs:choice>
                    <xs:element name="Errors" type="ErrorsType"/>
                    <xs:sequence>
                        <xs:element name="Report" type="ReportType" minOccurs="0"/>
                    </xs:sequence>
                </xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
