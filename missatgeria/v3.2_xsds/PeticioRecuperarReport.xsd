						<xs:choice>
							<xs:element name="IdNotificacio" type="xs:integer">
								<xs:annotation>
									<xs:documentation>Identificador de la notificaci√≥ a consultar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DataReport" type="xs:date">
								<xs:annotation>
									<xs:documentation>Data del dia del qual es vol obtenir el report.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
